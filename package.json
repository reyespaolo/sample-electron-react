{
  "name": "react-electron-starter",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "font-awesome": "^4.7.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lodash": "^4.17.10",
    "npm-run-all": "^4.1.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "primereact": "^1.5.2",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "radium": "^0.24.0",
    "raf": "3.4.0",
    "react": "^16.3.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.3.0",
    "react-transition-group": "^2.3.1",
    "rimraf": "^2.6.2",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "electron": "^1.7.10",
    "electron-builder": "^19.52.1",
    "foreman": "^2.0.0"
  },
  "scripts": {
    "dev": "nf start",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "electron": "electron .",
    "ebuild": "yarn private:clean build && node_modules/.bin/build",
    "pack:mac": "yarn run-s build private:build:mac",
    "pack:win": "yarn run-s build private:build:win",
    "pack:linux": "yarn run-s build private:build:linux",
    "pack:all": "yarn run-s build private:build:all",
    "private:clean": "yarn rimraf build",
    "private:build:mac": "electron-builder -m",
    "private:build:win": "electron-builder -w",
    "private:build:linux": "electron-builder -l",
    "private:build:all": "electron-builder -mwl"
  },
  "description": "React and Electron Awesome App",
  "homepage": "./",
  "main": "electron/main.js",
  "build": {
    "productName": "React Electron App",
    "appId": "com.reactbits.reactelectron",
    "electronVersion": "1.7.10",
    "directories": {
      "buildResources": "."
    },
    "files": [
      "build/**/*",
      "electron/*"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "publish": [
        "github"
      ]
    },
    "win": {},
    "linux": {
      "category": "Network"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
